  "version": "0.2.0",
  "configurations": [
    {
      "name": "Node.js - Debug Current File",
      "type": "node",
      "request": "launch",
      "program": "${file}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Node: Nodemon",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "debug"],
      "outputCapture": "std",
    }
  ]
}